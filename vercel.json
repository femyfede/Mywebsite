{
    "version": 2,
    "builds": [
        {
            "src": "mywebsite/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    
    "routes": [
        {
             "src": "/static/(.*)", 
            "dest": "mywebsite/wsgi.py"
        }
    ]
    
}
